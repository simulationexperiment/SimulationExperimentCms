<div class="breadcrumbs" id="breadcrumbs">
  <ul class="breadcrumb">
    <li>
      <i class="icon-home home-icon"></i>
      <a href="/index">主页</a>
    </li>
    <li>
      账户信息
    </li>
    <li class="active">密码修改</li>
  </ul>
</div>

<div class="page-content" ng-app="myApp" ng-controller="myCtrl">
  <div class="page-header">
    <h1>
      密码修改
      <small>
        <i class="icon-double-angle-right"></i>
        详细信息
      </small>
    </h1>
  </div>
  <div class="space-6"></div>
  <div class="row">
    <div class="col-xs-12">

      <form class="form-horizontal" name="myForm" novalidate>
        <div class="form-group">
          <label class="col-sm-2 control-label">原始密码</label>
          <div class="col-sm-8">
            <input type="password" class="form-control" placeholder="请输入原始密码">
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label">新密码</label>
          <div class="col-sm-8">
            <input type="password" class="form-control" placeholder="请输入新密码">
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-2 control-label">确认密码</label>
          <div class="col-sm-8">
            <input type="password" class="form-control" placeholder="请输入确认密码">
          </div>
        </div>


        <div class="modal-footer center">
          <button type="button" class="btn btn-primary" ng-disabled="model.selectedSystem.systemID == 0
                                                                            || model.selectedExercisesType.exercisesTypeCode == 0
                                                                            || model.selectedCourse.courseCode == 0
                                                                            || model.selectedKnowledge.knowledgeCode == 0
                                                                            || model.selectedExperiment.experimentCode == 0"> <i class="icon-save"></i>提交</button>
<!--          <button type="button" class="btn btn-sm btn-default" data-dismiss="modal"><i class="icon-remove"></i>关闭</button>-->
        </div>
      </form>

    </div>
  </div>


</div>

<input type="hidden" value="{{currentPageNum}}" id="hidden-currentPageNum">
<script src="/javascripts/view/assignExperiment.js"></script>